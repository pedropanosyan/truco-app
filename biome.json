{
	"$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"formatter": {
		"enabled": true,
		"indentWidth": 1
	},
	"linter": {
		"enabled": true,
		"rules": {
			"complexity": {
				"noForEach": "off"
			},
			"style": {
				"noNonNullAssertion": "off"
			}
		}
	},
	"organizeImports": {
		"enabled": true
	},
	"files": {
		"ignore": ["node_modules/**", "dist/**", "tsconfig.*"]
	}
}
